{% extends 'base.html.twig' %}

{% block title %}TblTipoDocumento index{% endblock %}

{% block body %}
    <h1>TblTipoDocumento index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>IdTipo</th>
                <th>Tipo</th>
                <th>Estado</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for tbl_tipo_documento in tbl_tipo_documentos %}
            <tr>
                <td>{{ tbl_tipo_documento.idTipo }}</td>
                <td>{{ tbl_tipo_documento.tipo }}</td>
                <td>{{ tbl_tipo_documento.estado }}</td>
                <td>
                    <a href="{{ path('tbl_tipo_documento_show', {'idTipo': tbl_tipo_documento.idTipo}) }}">show</a>
                    <a href="{{ path('tbl_tipo_documento_edit', {'idTipo': tbl_tipo_documento.idTipo}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="4">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('tbl_tipo_documento_new') }}">Create new</a>
{% endblock %}
