<form method="post" action="{{ path('tbl_numeracion_delete', {'idNumeracion': tbl_numeracion.idNumeracion}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ tbl_numeracion.idNumeracion) }}">
    <button class="btn">Delete</button>
</form>
