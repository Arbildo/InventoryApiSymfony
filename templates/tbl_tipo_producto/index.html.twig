{% extends 'base.html.twig' %}

{% block title %}TblTipoProducto index{% endblock %}

{% block body %}
    <h1>TblTipoProducto index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>IdTipo</th>
                <th>Nombre</th>
                <th>Estado</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for tbl_tipo_producto in tbl_tipo_productos %}
            <tr>
                <td>{{ tbl_tipo_producto.idTipo }}</td>
                <td>{{ tbl_tipo_producto.nombre }}</td>
                <td>{{ tbl_tipo_producto.estado }}</td>
                <td>
                    <a href="{{ path('tbl_tipo_producto_show', {'idTipo': tbl_tipo_producto.idTipo}) }}">show</a>
                    <a href="{{ path('tbl_tipo_producto_edit', {'idTipo': tbl_tipo_producto.idTipo}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="4">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('tbl_tipo_producto_new') }}">Create new</a>
{% endblock %}
